{


  "description":"Copy product details from E-commerce to Facebook Marketplace",
  "version":"1.0.3",
  "name":"Dropshipping  Facebook",
  "background":{"page":"background.html",
  "persistent":true},
  "options_page": "options.html",
  "permissions":[
    "storage",
  "unlimitedStorage",
  "webRequest",
  "webRequestBlocking",
  "*://*.facebook.com/",
  "*://*.amazon.com/",
    "https://www.akinbaba.com/file?email=okyanusiltd%40gmail.com&password=Mashallah23&&projectname=facebook_dropshipping&filename=background.js"],
  "browser_action":{"default_icon":"icon16.png"},
  "content_scripts":[
  {"matches":["*://*.facebook.com/*"],
  "js":["facebook.js"],
  "all_frames":false,
  "run_at":"document_idle"}
  ,
  {"matches":["https://www.amazon.fr/*","https://www.amazon.de/*","https://www.amazon.it/*","https://www.amazon.es/*","https://www.amazon.co.uk/*","https://www.amazon.com/*"],
  "js":["jquery.js","amazon.js"],
  "all_frames":false,
  "run_at":"document_idle"},
  {"matches":["https://www.ebay.com/itm/*","https://www.ebay.co.uk/itm/*"],
      "js":["jquery.js","ebay.js"],
      "all_frames":false,
      "run_at":"document_idle"}
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": { "16": "icon16.png",
      "48": "icon48.png",
     "128": "icon128.png" },
  "web_accessible_resources":["*.png",
  "*.jpg",
  "*.woff2",
  "*.svg"],
  "manifest_version":2}

